[Unit]
Description=rpstir2 service
After=mysqld.service
#Requires=mysqld.service
[Service]
Type=forking
WorkingDirectory=/root/rpki/rpstir2/bin
ExecStart=/root/rpki/rpstir2/bin/rpstir2-crontab.sh start
#ExecStop=/root/rpki/rpstir2/bin/rpstir2-crontab.sh stop
#Restart=always
[Install]
WantedBy=multi-user.target